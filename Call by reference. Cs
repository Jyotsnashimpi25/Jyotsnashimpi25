using System;

    class jyo
    {
    	void show(ref int x)
    	{
    		x=10;
    	Console.WriteLine(x);
    	}
    public static void Main(string[] args)
    {
    	 int x=20;
    	jyo p1=new jyo();
    	Console.WriteLine("before function call"+x);
    	p1.show(ref x);
    	Console.WriteLine("after call "+x);
    }
    }
       
